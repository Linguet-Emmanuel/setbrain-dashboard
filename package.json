{
  "name": "setbrain-dashboard",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "e2e": "playwright test apps/e2e"
  },
  "devDependencies": {
    "@testing-library/react": "14.0.0",
    "config": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "^1.10.12"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.27.1"
}