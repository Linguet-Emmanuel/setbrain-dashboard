{
  "name": "shared-data-access-projects",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/data-access/projects/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/data-access/projects",
        "tsConfig": "libs/shared/data-access/projects/tsconfig.lib.json",
        "packageJson": "libs/shared/data-access/projects/package.json",
        "main": "libs/shared/data-access/projects/src/index.ts",
        "assets": ["libs/shared/data-access/projects/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/data-access/projects/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/data-access/projects/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
