{
  "name": "shared-data-access-tasks",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/data-access/tasks/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/data-access/tasks",
        "tsConfig": "libs/shared/data-access/tasks/tsconfig.lib.json",
        "packageJson": "libs/shared/data-access/tasks/package.json",
        "main": "libs/shared/data-access/tasks/src/index.ts",
        "assets": ["libs/shared/data-access/tasks/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/shared/data-access/tasks/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/data-access/tasks/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
